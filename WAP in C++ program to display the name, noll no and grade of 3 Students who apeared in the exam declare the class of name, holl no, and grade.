#include<iostream>

using namespace std;

int main()

{

    char name[40];

    char course[20];

    int roll;

    cout<<"Enter Name:\n";

    cin>>name;

    cout<<"Enter Course:\n";

    cin>>course;

    cout<<"Enter Roll:\n";

    cin>>roll;



  cout<<"\nYour name is:"<<name;

  cout<<"\nYour course is:"<<course;

  cout<<"\nYour roll is :"<<roll;

cout<<"\nThank you";

}



